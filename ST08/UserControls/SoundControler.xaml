<UserControl x:Class="PA.Stavadlo.UserControls.SoundControler"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PA.Stavadlo.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="35" d:DesignWidth="35"
              Width="35" Height="35">
    <Grid>
        <ToggleButton Margin="0" Name="SoundButton"  Style="{StaticResource soundButton}" Click="SoundButton_Click" />
    </Grid>
</UserControl>
