    <!--
        Zobrazuje schemu pre Stavadlo08;
        Blend for Visual Studio 2022 Community nefunguje dobre!!!
        Preto schemu nakreslim tu Blend 2019 a dufam ze to skoro v Microsofte opravia
    
    SEMAFORY:
    SemaphoreType udava aky typ semaforu sa zobrazi 
     S1 - Klasicky stojanovy semafor s nozickou a podstavou (zelena/cervena).
     S2 - Zriadovaci  Sexx  len s podstavou (biela/modra).
     S3 - Trpaslici semafor s  podstavou (zelena/cervena).
     S4 - Zriadovaci (Klasicky stojanovy) semafor s nozickou a podstavou (biela/modra).

    ToolTip pre semafor zobrazuje cislo semaforu, pozri Semafor1Control.xaml
    -->
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/StavadloElementsStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <!--konvertery-->

            <conv:BackgroundColorConverter2 x:Key="BackColorConv"/>
            <conv:BackgroundColorConverter3 x:Key="BackColorConv3"/>
            <!--nastavuje pozadie pre nazov neizolovaneho useku-->
            <conv:SwitchNameColorConverter x:Key="ForeSwitchNameConv"/>
            <conv:TestConverter x:Key="testConv" />
            <conv:SytostFarbyConverter x:Key="sytostConv"/>

            <!--Styl pre usek, event setter pre lavy klik na usek-->
            <Style TargetType="{x:Type Path}"  BasedOn="{StaticResource basicPath}" >
                <EventSetter Event="UIElement.MouseLeftButtonDown" Handler="PathMouseLeftButtonDown"/>
            </Style>

            <Style TargetType="{x:Type Rectangle}">
                <Setter Property="Fill" Value="Transparent"/>
                <Setter Property="VerticalAlignment" Value="Top"/>
                <Setter Property="HorizontalAlignment" Value="Left"/>
                <Setter Property="Stroke" Value="Black"/>
                <Setter Property="StrokeThickness" Value="2"/>
                <Setter Property="RadiusX" Value="0"/>
                <Setter Property="RadiusY" Value="0"/>
                <Setter Property="RenderTransformOrigin" Value="0.5 0.5"/>
            </Style>

            <Style TargetType="{x:Type TextBox}" x:Key="TXB1">
                <!--Set to true to not get any properties from the themes.-->
                <Setter Property="OverridesDefaultStyle" Value="True"/>
                <Setter Property="IsReadOnly" Value="True"/>
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type TextBox}">
                            <Grid>
                                <Border BorderBrush="Black" BorderThickness="2"/>
                                <ContentPresenter HorizontalAlignment="Center"
                                VerticalAlignment="Center"/>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>


        </ResourceDictionary>

    </UserControl.Resources>

